@page "/counter"
@inject Test testService
@rendermode InteractiveServer

<PageTitle>Counter</PageTitle>

<h1>Counter</h1>

<p role="status">Current count: @currentCount</p>

<button class="btn btn-primary" @onclick="IncrementCount">Click me 111</button>


@code {

    private void IncrementCount()
    {
        currentCount++;
        testService.SaveInteger(currentCount);
    }
}
